<root>
	<panel name="dicetower" modes="host,client">
		<bounds>,0,0,0</bounds>
		<anchored height="110" width="110">
			<bottom offset="-35" />
			<right parent="shortcutsanchor" anchor="left" />
		</anchored>
		<class>dicetower</class>
		<dynamic />
		<locked />
	</panel>

	<panel name="ringstain" modes="all">
		<bounds>0,0,0,0</bounds>
		<anchored height="180" width="180">
			<bottom offset="-50" />
			<right parent="dicetower" anchor="left" offset="-10"/>
		</anchored>
		<class>ringstain</class>
		<dynamic />
		<locked />
	</panel>
</root>